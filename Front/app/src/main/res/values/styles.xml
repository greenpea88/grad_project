<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorFastWithUs5</item>
        <item name="colorPrimaryDark">@color/colorFastWithUs1</item>
        <item name="colorAccent">@color/colorFastWithUs3</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    
    <style name="ToolbarDark" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="android:textColor">#fff</item>
        <item name="colorPrimary">@color/colorFastWithUs1</item>
    </style>

    <style name="ToolbarTitle" parent="Base.TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">#fff</item>
    </style>

</resources>